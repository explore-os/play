{
  "name": "Actor Playground",
  "dockerComposeFile": [
    "docker-compose.yml"
  ],
  "service": "eos",
  "runServices": ["msgbus", "eos"],
  "workspaceFolder": "/explore",
  "customizations": {
    "vscode": {
      "settings": {
        "terminal.integrated.defaultProfile.linux": "fish",
        "files.exclude": {
          "/explore/persist/nats": true,
          "**/.devcontainer": true,
          "**/.git": true,
          "**/.svn": true,
          "**/.hg": true,
          "**/.DS_Store": true,
          "**/Thumbs.db": true
        }
      },
      "extensions": [
        "udoprog.rune-vscode"
      ]
    }
  }
}